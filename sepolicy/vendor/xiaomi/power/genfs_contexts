# Double Tap Sensor
genfscon proc /tp_gesture u:object_r:proc_tp_gesture:s0
genfscon sysfs /devices/platform/soc/4a88000.i2c/i2c-1/1-0038/double_tap_pressed u:object_r:sysfs_touchpanel:s0
genfscon sysfs /touchpanel u:object_r:sysfs_touchpanel:s0

# Single Tap Sensor
genfscon sysfs /devices/platform/soc/4a88000.i2c/i2c-1/1-0038/single_tap_pressed u:object_r:sysfs_touchpanel:s0
